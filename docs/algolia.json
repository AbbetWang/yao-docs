{
  "index_name": "yao-docs",
  "start_urls": ["https://wwsheng009.github.io/yao-docs"],
  "rateLimit": 8,
  "maxDepth": 10,
  "selectors": {
    "lvl0": {
      "selector": "",
      "defaultValue": "Documentation"
    },
    "lvl1": ".content h1",
    "lvl2": ".content h2",
    "lvl3": ".content h3",
    "lvl4": ".content h4",
    "lvl5": ".content h5",
    "lang": {
      "selector": "/html/@lang",
      "type": "xpath",
      "defaultValue": ["zh-CN"]
    },
    "content": ".content p, .content li"
  },
  "selectors_exclude": [
    "aside",
    ".page-footer",
    ".next-and-prev-link",
    ".table-of-contents"
  ],
  "custom_settings": {
    "attributesForFaceting": ["lang"]
  },
  "js_render": true
}
